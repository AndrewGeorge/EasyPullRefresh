<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.anumbrella.easypullrefresh">


    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Activity.ListView.PullRefreshListViewActivity" />

        <activity android:name=".Activity.RecyclerView.PullRefreshRecyclerViewActivity" />

        <activity android:name=".Activity.RecyclerView.PullRefreshRecyclerListViewActivity" />

        <activity android:name=".Activity.RecyclerView.PullRefreshRecyclerGridViewActivity" />

        <activity android:name=".Activity.WebView.PullRefreshWebViewActivity" />

        <activity android:name=".Activity.ListView.DefaultStyleActivity" />

        <activity android:name=".Activity.ListView.CustomStyleActivity"/>

        <activity android:name=".Activity.ListView.IndicatorStyleActivity"/>

        <activity android:name=".Activity.ListView.SettingStyleActivity"/>

    </application>

</manifest>